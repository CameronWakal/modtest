<div class="module-ports">{{#each module.enabledPorts as |port|}}{{component 
  port.constructor.modelName 
  port=port 
  startedConnecting=(action 'portStartedConnecting' port)
  finishedConnecting=(action 'portFinishedConnecting')
  mouseEnter=(action 'mouseEnterPort' port)
  mouseLeave=(action 'mouseLeavePort') 
  disconnect=(action 'disconnectPort' port)
}}{{/each}}</div>